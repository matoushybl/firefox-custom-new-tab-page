{
  "manifest_version": 2,
  "name": "My New Tab Page",
  "version": "0.7.0",
  "description": "Show a new tab page with urls of my choice",
  "developer": {
    "name": "MatousHybl",
    "url": "https://github.com/matoushybl/firefox-custom-new-tab-page"
  },
  "icons": {
    "48": "img/icon-48.png",
    "96": "img/icon-96.png"
  },
  "applications": {
    "gecko": {
      "id": "custom-new-tab-page@mint.as",
      "strict_min_version": "57.0"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides" : {
    "newtab": "app.html"
  }
}
